{
    "dns": {
        "hosts": {
			"dns.cloudflare.com": "cloudflare.com",
            "dns.electrotm.org": "78.157.42.100",
            "sky.rethinkdns.com": "188.114.97.3",
            "cvt01.dnscry.pt": [
                "45.155.37.165"
            ]
        },
        "servers": [
			"https://dns.cloudflare.com/dns-query",
            {
                "address": "1.1.1.1",
                "domains": [
                    "geosite:private",
                    "geosite:category-ir",
                    "full:cloudflare.com"
                ]
            },
            "https://cvt01.dnscry.pt/dns-query",
            "https://sky.rethinkdns.com/",
            {
                "address": "https://dns.electrotm.org/dns-query",
                "domains": [
                    "domain:googleapis.com",
                    "geosite:spotify"
                ],
                "skipFallback": true
            }
        ],
        "queryStrategy": "UseIP"
    },
    "log": {
        "error": "errors-sni.log",
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 443,
            "protocol": "dokodemo-door",
            "settings": {
                "network": "tcp",
                "followRedirect": true
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "tls",
                    "http",
                    "quic"
                ]
            },
            "streamSettings": {
                "sockopt": {
                    "tproxy": "tproxy"
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "free",
            "settings": {
                "domainStrategy": "UseIP",
                "fragment": {
                    "packets": "tlshello",
                    "length": "10-15",
                    "interval": "0-0"
                }
            },
            "streamSettings": {
                "sockopt": {
                    "tcpNoDelay": true
                }
            }
        }
    ],
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
        ]
    }
}